     internal_properties : CAAAAAgAAABAqJXZhIfxjz45BJ7nWkNs6+790xSFJi2MXBC0vz/GD74WJh49btzw1rQ10kJPc0kAkW9axbmp7GPmAqRcjuLklyPYNXTd+QVkoxXJxz8H70D7bKDsp4gXlzSvd1xl0bS+dR5e0ya2YVGMFyjJK7jhfMpAOcd4X4R5ZpWNylI+N4Ks7p7f7DEcyucJH7L+B/2hMUQxCCz6sKEI7pT/qlTcrmoCXrYj
     procedure_id : 1433486863102752141
     type_code : 14
     code : |1+
      // Summary: Creates a new photo
      //
      procedure NewPhoto()
      
      // Opens camera
      OpenAsynchronous(WIN_Mobile_Camera_UI,(False),AfterCapture)
      
      	INTERNAL PROCEDURE AfterCapture(sPath is string)
      		sDestinationName is string
      		
      		// If the return is an empty string vide ("")
      		if sPath ~= "" then
      			return
      		end
      		
      		// Future name of photo file
      		sDestinationName  = StringBuild("%1-%2%3",SysDateTime() ,GetGUID(guidRough), fExtractPath(sPath,fExtension))
      		
      		// Copy the image into the application directory
      		fCopyFile(sPath,PhotoPath(sDestinationName))
      		
      		// Generate the miniature
      		IMG_Miniature = sPath
      		if Lower(fExtractPath(sPath,fExtension)) = ".png" then
      			dSaveImagePNG(IMG_Miniature,ThumbnailPhotoPath(sDestinationName))
      		else
      			dSaveImageJPEG(IMG_Miniature,ThumbnailPhotoPath(sDestinationName))
      		end
      		
      		// Delete the source image 
      		fDelete(sPath)
      		
      		// Add into the database
      		HReset(Photo)
      		Photo.Path			= sDestinationName
      		Photo.CreationDateTime	= SysDateTime()
      		HAdd(Photo)
      		
      		// Display the photo
      		OpenMobileWindow(WIN_Photo,Photo.PhotoID)
      		
      		// Refresh
      		RefreshList()
      	end
      
