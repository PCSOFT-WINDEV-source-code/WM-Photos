     code : |1-
      // Summary: Refreshes the list of photos
      //
      procedure RefreshList()
      
      LooperDeleteAll(LOOP_Photos)
      
      // Run the query
      QRY_Photos.pSearch = gsFind
      HExecuteQuery(QRY_Photos)
      
      for each QRY_Photos 
      	// Add the miniature into the looper
      	LooperAddLine(LOOP_Photos,QRY_Photos.PhotoID,ThumbnailPhotoPath(QRY_Photos.Path))
      end
