  p_codes :
   -
     code : |1+
      procedure MyWindow(local bAllowVideo = True)
      
      // Initializes the initial plane to be displayed
      CAM_Capture.Camera=camBackCamera
      CAM_Capture.Zoom=1
      CAM_Capture.Flash=camFlashAuto
      CAM_Capture.Torch=camTorchOff
      MyWindow.Plane=PLAN_Photo_Capture
      
      // Displays the image to switch to video mode if available, or hides the image if video recording is not allowed
      IMG_VideoMode.Visible = bAllowVideo
      
      // Identifier of the timer that periodically calls OnElapsedTime
      gnTimerVideoElapsedTime is int = -1
      
      // Path of the image/video that will be generated
      gsPath is string
   -
     code : |1+
      // Initialize the UI on the 1st display
      // Note: The different elements of the UI have automatically added internal procedures notified when RequestRefreshUI() was called
      RequestRefreshUI()
     type : 34
   -
     code : |1+
      // Calculate the elapsed time in a video recording
      dtStartRecording is datetime
      
      // Applies a format to the elapsed time if a video recording is in progress
      if MyWindow.Plane = PLAN_Recording_in_progress then
      	if gnTimerVideoElapsedTime = -1 then		
      	 	// Formats the elapsed time for each timer call
      	 	gnTimerVideoElapsedTime = TimerSys(OnElapsedTime,250 ms)
      	 	INTERNAL PROCEDURE OnElapsedTime
      	 		dtNow is datetime
      	 		durationElapsed is duration = dtNow - dtStartRecording		
      	 		STC_ElapsedTime = DurationToString(durationElapsed,"HH:MM:SS")
      	 	end
      	 	// 1st call to initialize the elapsed time
      	 	OnElapsedTime()
      	end	
      // Stops calculating the elapsed time
      else if gnTimerVideoElapsedTime>-1
      	EndTimerSys(gnTimerVideoElapsedTime)
      	gnTimerVideoElapsedTime = -1
      end
      
      // if the flashlight is on and you quit video recording, turn off the flashlight
      if CAM_Capture.Torch=camTorchOn _and_ MyWindow.Plane<>PLAN_Video_Recording _and_ MyWindow.Plane<>PLAN_Recording_in_progress  then
      	CAM_Capture.Torch=camTorchOff
      end
      
     type : 234
   -
     type : 180
     enabled : false
   -
     type : 230
     enabled : false
   -
     code : |1-
      // Turns off the flashlight when the window is closed
      CAM_Capture.Torch=camTorchOff
      
      // Returns the path of the captured image or video to the calling window 
      return gsPath
     type : 2
  procedures :
   -
     name : TakePhoto
     procedure_id : 1433486867398769059
     type_code : 14
     code : |1-
      // Takes a photo from the Camera control
      procedure TakePhoto()
      
      CameraPhoto(CAM_Capture, OnPhotoCapture)
      INTERNAL PROCEDURE OnPhotoCapture(bOK, sPhotoPath)
      	if not bOK then
      		ToastDisplay(<§Rff00009b8fac5d7f¤en-GB¤Error taking the photo.§> + [CR] + ErrorInfo())
      		return False
      	end
      	// Stores the path of the photo taken
      	gsPath = sPhotoPath
      	IMG_Preview=sPhotoPath
      	MyWindow.Plane = PLAN_Photo_preview
      	// Refreshes the display
      	RequestRefreshUI()
      	
      	return True
      end
      
      // Note: Here, it is possible to customize the display during the current photo capture
     type : 458752
   -
     name : StartRecordingVideo
     procedure_id : 1433486867398834595
     type_code : 14
     code : |1-
      procedure StartRecordingVideo()
      
      // Starts recording a video from the Camera control
      CameraVideoStart(CAM_Capture,OnEndVideoRecording)
      
      INTERNAL PROCEDURE OnEndVideoRecording(bOK, sVideoPath)
      	if not bOK then
      		ToastDisplay(<§Rff00019b8fae5d7f¤en-GB¤Error recording the video.§> + [CR] + ErrorInfo())
      		return False
      	end
      	// Memorize the path of the video taken
      	gsPath	= sVideoPath
      	MM_Preview=gsPath
      	MyWindow.Plane	= PLAN_Video_preview
      	// Refreshes the display
      	RequestRefreshUI()
      	return True
      end
      
      // Displays the current record
      MyWindow.Plane = PLAN_Recording_in_progress
      RequestRefreshUI()
     type : 458752
   -
     name : ModifyZoom
     procedure_id : 1433486867398900131
     type_code : 14
     code : |1+
      procedure ModifyZoom(rButtonZoomValue)
      
      // Refresh UI when zoom is applied
      if CAM_Capture.Zoom<>rButtonZoomValue then
      	CAM_Capture.Zoom=rButtonZoomValue	
      end
      
      STC_ZOOM.FormatZoomText(CAM_Capture.Zoom)
     type : 458752
