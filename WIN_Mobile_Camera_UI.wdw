        type : 14
     procedures :
      -
        name : FormatZoomText
        internal_properties : CAAAAAgAAADaa/V/OxRssS4Cz7jPSUvuaDHW1cr3mTw1G1qUkAGQd7bD8fE3u/GkabZreVhw1insOwVgOmStDFuLGOKLKhWIDesPuNwy2+7r5CqVx1qjKM9MByrBFOO4+hQBol8yswKnJJ4lhkvq05SW/uUB
        procedure_id : 1433486867399031203
        type_code : 14
        code : |1+
         // Applies the zoom in the Camera control and shows the zoom text if the zoom value is different from the initial value
         procedure FormatZoomText(rZoom, local bMakeVisibleIfNecessary=True)
         // Checks if the value has changed
         let sNewZoomText = StringBuild(<§R11eab9cc1614bbac¤en-GB¤%1x§>, Round(rZoom,1))
         // If the value has changed, show zoom
         if bMakeVisibleIfNecessary _and_ sNewZoomText=MyControl..Caption then
         	bMakeVisibleIfNecessary=False
         end
         // Updates the text
         MyControl..Caption=sNewZoomText
         // if the text was invisible and must be made visible, do it immediately
         if bMakeVisibleIfNecessary then
         	MyControl..VisibleWithAnimation=True
         	if gnInvisibleTimer<>0 then 
         		EndTimerSys(gnInvisibleTimer)
         	end
         	gnInvisibleTimer=TimerSys(HideAfterTimeout, 1 s)
         end
         
         // Hides the control after a given time if no other zoom has been applied
         INTERNAL PROCEDURE HideAfterTimeout()
         	EndTimerSys(gnInvisibleTimer)
         	gnInvisibleTimer=0
         	MyControl..VisibleWithAnimation=False
         end
        type : 458752
