     code : |1-
      // Summary: Deletes the photo
      //
      procedure DeletePhoto()
      
      HReadSeekFirst(Photo,PhotoID,gnPhotoID)
      if HFound(Photo) then
      	// Delete the physical files
      	fDelete(PhotoPath(Photo.Path))
      	fDelete(ThumbnailPhotoPath(Photo.Path))
      	
      	// Deletion from the database
      	HDelete(Photo)
      	
      end
      	
      MyWindow.ReturnedValue = "del"	
      Close
